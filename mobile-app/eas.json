{
  "cli": {
    "version": ">= 16.19.1",
    "appVersionSource": "remote"
  },
  "build": {
      "preview": {
        "environment": "preview",
        "distribution": "internal",
        "developmentClient": false, 
        "android": { 
          "buildType": "apk",
          "gradleCommand": ":app:assembleRelease"
        },
        "ios": {
          "buildConfiguration": "Release"
        }
      },
      "preview2": {
        "environment": "preview",
        "distribution": "internal",
        "developmentClient": true, 
        "android": { 
          "buildType": "apk",
          "gradleCommand": ":app:assembleDebug"
        }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}


